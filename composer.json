{
	"name": "tarlepp/symfony-flex-backend",
	"description": "Simple JSON API which is build on top of Symfony framework",
	"version": "0.2.1",
	"type": "project",
	"keywords": [
		"Symfony",
		"Flex",
		"API"
	],
	"homepage": "https://github.com/tarlepp/symfony-flex-backend",
	"license": "MIT",
	"authors": [
		{
			"name": "Tarmo Lepp√§nen",
			"email": "tarmo.leppanen@protacon.com",
			"homepage": "https://github.com/tarlepp",
			"role": "developer"
		}
	],
	"support": {
		"issues": "https://github.com/tarlepp/symfony-flex-backend/issues"
	},
	"prefer-stable": true,
	"require": {
		"php": "^7.1.3",
		"doctrine/annotations": "1.5.0",
		"doctrine/doctrine-fixtures-bundle": "2.4.0",
		"doctrine/doctrine-migrations-bundle": "1.2.1",
		"easycorp/easy-log-handler": "1.0.2",
		"lexik/jwt-authentication-bundle": "2.4.1",
		"matthiasnoback/symfony-console-form": "2.3.0",
		"nelmio/api-doc-bundle": "dev-master",
		"piwik/device-detector": "3.8.2",
		"ramsey/uuid": "3.7.1",
		"roave/security-advisories": "dev-master",
		"sensio/framework-extra-bundle": "4.0.0",
		"stof/doctrine-extensions-bundle": "1.2.2",
		"symfony/asset": "3.3.10",
		"symfony/console": "3.3.10",
		"symfony/expression-language": "3.3.10",
		"symfony/flex": "1.0.29",
		"symfony/form": "3.3.10",
		"symfony/framework-bundle": "3.3.10",
		"symfony/monolog-bundle": "3.1.1",
		"symfony/orm-pack": "1.0.3",
		"symfony/security": "3.3.10",
		"symfony/security-bundle": "3.3.10",
		"symfony/serializer": "3.3.10",
		"symfony/twig-bundle": "3.3.10",
		"symfony/validator": "3.3.10",
		"symfony/yaml": "3.3.10"
	},
	"require-dev": {
		"liuggio/fastest": "1.5.0",
		"phpmetrics/phpmetrics": "2.3.0",
		"phpunit/phpunit": "6.4.1",
		"satooshi/php-coveralls": "1.0.1",
		"sensiolabs/security-checker": "4.1.5",
		"squizlabs/php_codesniffer": "3.1.0",
		"symfony/browser-kit": "3.3.10",
		"symfony/dotenv": "3.3.10",
		"symfony/phpunit-bridge": "3.3.10",
		"symfony/requirements-checker": "1.0.2",
		"symfony/web-profiler-bundle": "3.3.10",
		"symfony/web-server-bundle": "3.3.10"
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"auto-scripts": {
			"make cache-warmup": "script",
			"assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
			"vendor/bin/security-checker security:check --end-point=http://security.sensiolabs.org/check_lock": "php-script",
			"requirements-checker": "script"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		],
		"metrics": "./vendor/bin/phpmetrics --report-html=build/phpmetrics .",
		"test": "./vendor/bin/phpunit",
		"test-parallel": "./vendor/bin/fastest -x phpunit.xml.dist"
	},
	"conflict": {
		"symfony/symfony": "*",
		"symfony/twig-bundle": "<3.3",
		"symfony/debug": "<3.3"
	},
	"extra": {
		"projectTitle": "Symfony Flex Backend",
		"symfony": {
			"id": "01BFA135DXTPRA5CR250NTJJJP",
			"allow-contrib": "true"
		}
	}
}
