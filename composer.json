{
    "name": "tarlepp/symfony-flex-backend",
    "description": "Simple JSON API which is build on top of Symfony framework",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Tarmo Lepp√§nen",
            "homepage": "https://github.com/tarlepp",
            "role": "developer"
        }
    ],
    "support": {
        "issues": "https://github.com/tarlepp/symfony-flex-backend/issues"
    },
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.4@dev",
        "ramsey/uuid": "^3.0@dev",
        "roave/security-advisories": "dev-master",
        "sensio/framework-extra-bundle": "^4.0@dev",
        "symfony/console": "^3.3@dev",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3@dev",
        "symfony/monolog-bundle": "^3.0@dev",
        "symfony/orm-pack": "^2.4@dev",
        "symfony/security-bundle": "^3.3@dev",
        "symfony/serializer": "^3.3@dev",
        "symfony/twig-bundle": "^3.3@dev",
        "symfony/validator": "^3.3@dev",
        "symfony/web-server-bundle": "^3.3@dev",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1@dev",
        "satooshi/php-coveralls": "^2.0@dev",
        "sensiolabs/security-checker": "^4.0@dev",
        "symfony/browser-kit": "^3.3@dev",
        "symfony/dotenv": "^3.3",
        "symfony/phpunit-bridge": "^3.3@dev",
        "symfony/requirements-checker": "^1.0@dev",
        "symfony/web-profiler-bundle": "^3.3@dev"
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %WEB_DIR%": "symfony-cmd",
            "vendor/bin/security-checker security:check --end-point=http://security.sensiolabs.org/check_lock": "php-script",
            "%BIN_DIR%/check.php": "php-script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "<3.3",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BFA135DXTPRA5CR250NTJJJP",
            "allow-contrib": false
        }
    }
}
